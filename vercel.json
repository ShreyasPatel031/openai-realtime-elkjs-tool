{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "outputDirectory": "client/dist/client",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/token",
      "dest": "/api/token"
    },
    {
      "src": "/stream",
      "dest": "/api/stream"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/icons/$1"
    },
    {
      "src": "/group-icons/(.*)",
      "dest": "/group-icons/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 