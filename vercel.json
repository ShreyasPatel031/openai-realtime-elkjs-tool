{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run generate-icons && npm run generate-dynamic-lists && vite build",
  "outputDirectory": "dist",
  "installCommand": "npm install",

  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 